{
  "app": {
    "background": {
      "scripts": ["src/app.js"]
    }
  },
  "manifest_version": 2,
  "name": "MyAppName",
  "version": "versionString",
  "default_locale": "en",
  "description": "MyAppName: MyAppDescription",
  "short_name": "AppName",
  "start_url": "/app",
  "theme_color": "#FF4C00",
  "display": "standalone",
  "background_color": "#FF4C00",
  "icons": [
    {
      "src": "src/icons/logo 48.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "src/icons/logo 96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "src/icons/logo 144.png",
      "sizes": "144x144",
      "type": "image/png"
    }
  ]
}
